{
  "name": "taigi-dict-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mobile": "vite --config vite.config.js",
    "build": "vite build --config vite.config.web.js",
    "build:web": "vite build --config vite.config.web.js",
    "build:mobile": "vite build --config vite.config.js && npx cap sync",
    "preview": "vite preview --config vite.config.web.js",
    "preview:mobile": "vite preview --config vite.config.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:mobile": "VITE_PLATFORM=mobile vitest run",
    "ios": "npm run build:mobile && npx cap open ios",
    "android": "npm run build:mobile && npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:run:ios": "npm run build:mobile && npx cap run ios",
    "cap:run:android": "npm run build:mobile && npx cap run android",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run build:mobile && npm run analyze"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.1",
    "@capacitor/ios": "^7.4.1",
    "@supabase/supabase-js": "^2.46.1",
    "csv-writer": "^1.6.0",
    "dexie": "^4.0.8",
    "papaparse": "^5.4.1",
    "pinia": "^2.3.0",
    "pinyin": "^4.0.0-alpha.2",
    "segmentit": "^2.0.3",
    "vue": "^3.4.29",
    "vue-router": "^4.5.1",
    "zhuyin": "^1.14.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.1",
    "@capacitor/core": "^7.4.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "gh-pages": "^6.2.0",
    "happy-dom": "^18.0.1",
    "vite": "^5.3.1",
    "vitest": "^3.2.4"
  }
}
